function load_cogocast_pixels() { if(document.readyState == 'interactive'){setTimeout(load_cogocast_pixels, 100); return;}; if(! document.getElementsByTagName('body')[0]){var b = document.createElement('body'); document.getElementsByTagName('html')[0].appendChild(b);}; document_body = document.getElementsByTagName('body')[0]; (function(h){if(document.getElementById('cogocast_img_2')){return};var s=document.createElement('img');s.id='cogocast_img_2';s.src='//ce.lijit.com/merge?pid=5012&3pid=dbd8f8c1b7e94001b3a61e2ea148e120';s.style.display='none';s.onerror=function(){h.removeChild(s);};h.appendChild(s);})(document_body);
;}; load_cogocast_pixels();